  "author": "serguun42",
  "license": "BSL-1.0 License",
  "dependencies": {
    "jszip": "^3.10.1",
    "node-fetch": "^3.3.0",
    "node-html-parser": "^6.1.4",
    "pm2": "^5.2.2",
    "socks-proxy-agent": "^7.0.0",
    "tumblr.js": "^3.0.0",
    "twitter-lite": "^1.1.0",
    "yt-dlp-wrap": "^2.3.11"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
